def fre(x):
    frequency={}
    for char in x:
        if char in frequency:
            frequency[char]+=1
        else:
                frequency[char]=1
    for i in frequency:
        print("%s = %d"%(i,frequency[i]))
        
        
g= input("enter a string")
fre(g);



output

enter a stringayushi
a = 1
y = 1
u = 1
s = 1
h = 1
i = 1
