def fre(x):
    frequency_find={}
    for char in x:
        if char in frequency_find:
            frequency_find[char]+=1
        else:
                frequency_find[char]=1
    for i in frequency_find:
        print(i+"="+frequency_find[i])
        
        
g= input("enter a string")
fre(g);


output

enter a stringApplication
s=1
s=2
